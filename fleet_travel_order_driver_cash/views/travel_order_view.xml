            <field name="model">fleet.vehicle.travel.order</field>
            <field name="inherit_id" ref="fleet_travel_order.fleet_vehicle_travel_order_form" />
            <field name="arch" type="xml">
            <notebook>
                <page string="Driver Cash">
                    <field name="driver_cash_ids">
